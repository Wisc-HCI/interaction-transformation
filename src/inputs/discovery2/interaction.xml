<?xml version="1.0" encoding="utf-8"?>
<nta>

	<name>discovery_task</name>

	<group id="0" init="true" final="false" x="182.0" y="134.0">
		<name>Greeting</name>
		<micro>
			<name>Greeter</name>
			<instantiation>Greeting</instantiation>
		</micro>
	</group>

	<group id="1" init="false" final="false" x="182.0" y="134.0">
